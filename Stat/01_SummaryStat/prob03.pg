\( X\!: \langle 1, 2, 3, 4, 5, 6, 7, 8, 9 \rangle \)
$BR$BR

\( Y\!: \langle 1, 1, 2, 2, 5, 8, 8, 9, 9 \rangle \)
$BR$BR

\{ $mc1->print_q() \} $BR
\{ $mc1->print_a() \} $BR

\{ $mc2->print_q() \} $BR
\{ $mc2->print_a() \}

END_TEXT

$showPartialCorrectAnswers = 0;

ANS( radio_cmp( $mc1->correct_ans() ) );
ANS( radio_cmp( $mc2->correct_ans() ) );

