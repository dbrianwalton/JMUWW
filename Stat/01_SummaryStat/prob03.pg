DOCUMENT();

loadMacros("PGstandard.pl", "PGchoicemacros.pl");

$mc1=new_multiple_choice();
$mc1->qa(
"1. Which data set has the larger mean?",
"Both have the same mean."
);
$mc1->extra(
"\( X \)",
"\( Y \)"
);

$mc2=new_multiple_choice();
$mc2->qa(
"2. Which data set has the larger variance?",
"\( Y \)"
);
$mc2->extra(
"\( X \)",
"Both have the same variance."
);

BEGIN_TEXT

